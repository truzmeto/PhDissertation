	Number = {5},
	Pages = {420-33},
	Pmid = {15468065},
	Title = {Protein conformational transitions coupled to binding in molecular recognition of unstructured proteins: hierarchy of structural loss from all-atom Monte Carlo simulations of p27Kip1 unfolding-unbinding and structural determinants of the binding mechanism},
	Volume = {75},
	Year = {2004},
