	Mesh = {Amino Acid Sequence; Animals; Calcium; Calmodulin; Ligands; Models, Molecular; Molecular Sequence Data; Mutation; Phenylalanine; Protein Binding; Protein Structure, Tertiary; Rats; Spectrometry, Fluorescence; Thermodynamics; Time Factors; Tyrosine},
	Month = {Nov},
	Number = {5},
