	Journal = {J. Mol. Biol.},
	Journal-Full = {Journal of molecular biology},
	Mesh = {Animals; Calcium; Calmodulin; Cattle; Chromatography, Ion Exchange; Electrophoresis, Polyacrylamide Gel; Protein Conformation; Rabbits; Temperature; Thermodynamics; Troponin; Troponin C},
