	Doi = {10.1073/pnas.0901178106},
	Journal = {Proc Natl Acad Sci U S A},
	Journal-Full = {Proceedings of the National Academy of Sciences of the United States of America},
	Mesh = {Biotin; Coloring Agents; Fluorescence Resonance Energy Transfer; Immobilized Proteins; Kinetics; Light; Photons; Physical Phenomena; Protein Folding; Streptavidin; Time Factors; Urea},
	Month = {Jul},
	Number = {29},
	Pages = {11837-44},
	Pmid = {19584244},
