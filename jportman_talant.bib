	Mesh = {Hot Temperature; Kinetics; Models, Theoretical; Protein Denaturation; Protein Folding},
	Month = {Jan},
	Number = {2},
	Pages = {671-6},
	Pmid = {16405339},
	Title = {Effect of finite size on cooperativity and rates of protein folding},
	Volume = {110},
	Year = {2006},
