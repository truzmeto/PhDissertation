	Journal = {PLoS Comput Biol},
	Journal-Full = {PLoS computational biology},
	Mesh = {Arginine; Bacterial Proteins; Carrier Proteins; Holoenzymes; Markov Chains; Molecular Dynamics Simulation; Protein Binding; Salmonella typhimurium; Thermodynamics},
	Month = {May},
	Number = {5},
	Pages = {e1002054},
	Pmc = {PMC3102756},
	Pmid = {21637799},
