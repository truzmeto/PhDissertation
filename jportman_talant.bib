	Number = {5},
	Pages = {2864 - 2875},
	Title = {A Semiflexible Polymer Model Applied to Loop Formation in DNA Hairpins},
	Url = {http://www.sciencedirect.com/science/article/B94RW-4TXGN6H-1B/2/e5baf606191b193a1538545079b37501},
	Volume = {81},
