	Mesh = {Animals; Binding Sites; Biophysical Phenomena; Biophysics; Computer Simulation; Diffusion; Humans; Ligands; Macromolecular Substances; Protein Binding; Protein Conformation; Protein Folding; Proteins; Static Electricity; Thermodynamics},
	Month = {Mar},
	Number = {3},
	Pages = {1166-78},
	Pmid = {10049302},
	Title = {Free energy landscapes of encounter complexes in protein-protein association},
	Volume = {76},
	Year = {1999},
