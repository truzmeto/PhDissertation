	Journal = {Science},
	Journal-Full = {Science (New York, N.Y.)},
	Mesh = {Amino Acid Sequence; Animals; Binding Sites; Calcium; Calmodulin; Drosophila melanogaster; Hydrogen Bonding; Magnetic Resonance Spectroscopy; Models, Molecular; Molecular Sequence Data; Molecular Structure; Muscles; Myosin-Light-Chain Kinase; Peptide Fragments; Protein Conformation; Rabbits},
	Month = {May},
