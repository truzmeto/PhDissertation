	Mesh = {Computer Simulation; Kinetics; Ligands; Models, Molecular; Point Mutation; Protein Binding; Protein Conformation; Proteins; Thermodynamics},
	Month = {Apr},
	Number = {1},
	Pages = {104-10},
	Pmid = {18798570},
	Pst = {ppublish},
	Title = {Selected-fit versus induced-fit protein binding: kinetic differences and mutational analysis},
	Volume = {75},
