	Mesh = {Antigen-Antibody Complex; Computer Simulation; Dimerization; Models, Molecular; Pliability; Protein Binding; Protein Folding; Protein Structure, Quaternary; Protein Structure, Tertiary; Protein Subunits; Proteins; Water},
	Month = {Mar},
