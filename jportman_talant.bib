	Mesh = {CREB-Binding Protein; Cyclic AMP Response Element-Binding Protein; Magnetic Resonance Spectroscopy; Models, Molecular; Protein Binding; Protein Denaturation; Protein Folding; Protein Structure, Tertiary; Transcriptional Activation},
	Month = {Jun},
	Number = {7147},
	Pages = {1021-5},
	Pmid = {17522630},
	Title = {Mechanism of coupled folding and binding of an intrinsically disordered protein},
	Volume = {447},
	Year = {2007},
