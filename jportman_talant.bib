	Mesh = {Models, Molecular; Monte Carlo Method; Protein Binding; Protein Folding; Proteins},
	Month = {Apr},
	Number = {9},
	Pages = {5148-53},
	Pmid = {12697905},
	Title = {Simulating disorder-order transitions in molecular recognition of unstructured proteins: where folding meets binding},
	Volume = {100},
