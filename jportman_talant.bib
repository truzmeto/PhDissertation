@article{dyson:02,
	Abstract = {There are now numerous examples of proteins that are unstructured or only partially structured under physiological conditions and yet are nevertheless functional. Such proteins are especially prevalent in eukaryotes. In many cases, intrinsically disordered proteins adopt folded structures upon binding to their biological targets. Many new examples of coupled folding and binding events have been reported recently, providing new insights into mechanisms of molecular recognition.},
	Author = {Dyson, H Jane and Wright, Peter E},
	Date-Added = {2009-06-25 15:24:57 -0400},
