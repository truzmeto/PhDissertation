	Month = {Oct},
	Number = {5},
	Pages = {671-5},
	Pmid = {20832351},
	Pst = {ppublish},
	Title = {Enzymatic activity in disordered states of proteins},
	Volume = {14},
	Year = {2010},
	Bdsk-File-1 = {}}

@article{guedin:10,
	Abstract = {We compared here 80 different sequences containing four tracts of three guanines with loops of variable length (between 1 and 15 bases for unmodified sequences, up to 30 for fluorescently labeled oligonucleotides). All sequences were capable of forming stable quadruplexes, with Tm above physiological temperature in most cases. Unsurprisingly, the melting temperature was systematically lower in sodium than in potassium but the difference between both ionic conditions varied between 1 and >39$\,^{\circ}$C (average difference: 18.3$\,^{\circ}$C). Depending on the sequence context, and especially for G4 sequences involving two very short loops, the third one may be very long without compromising the stability of the quadruplex. A strong inverse correlation between total loop length and Tm was found in K+: each added base leads to a 2$\,^{\circ}$C drop in Tm or ∼0.3 kcal/mol loss in ΔG$\,^{\circ}$. The trend was less clear in Na+, with a longer than expected optimal loop length (up to 5 nt). This study will therefore extend the sequence repertoire of quadruplex-prone sequences, arguing for a modification of the widely used consensus (maximal loop size of 7 bases).

},
	Author = {Guedin, A. and Gros, J. and Alberti, P. and Mergny, J.L.},
	Date-Added = {2010-11-17 14:42:49 -0500},
